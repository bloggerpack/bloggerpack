{
  "name": "bloggerpack",
  "version": "1.0.0-beta.5",
  "version_current": "1.0.0-beta.5",
  "description": "A tool for develop Blogger theme.",
  "keywords": [
    "blogger",
    "blogspot",
    "theme",
    "template",
    "google"
  ],
  "author": "Igoy Nawamreh",
  "license": "MIT",
  "homepage": "https://github.com/bloggerpack/bloggerpack",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloggerpack/bloggerpack.git"
  },
  "bugs": {
    "url": "https://github.com/bloggerpack/bloggerpack/issues"
  },
  "bin": {
    "bloggerpack": "bin.js"
  },
  "scripts": {
    "test": "npm-run-all --serial test-lib test-tasks && mocha",
    "test-lib": "npm-run-all --serial test-lib-*",
    "test-lib-extract": "gulp build --gulpfile test/lib/extract/gulpfile.js",
    "test-lib-skin-ib": "gulp build --gulpfile test/lib/skin-import-beautifier/gulpfile.js",
    "test-lib-template-c": "gulp build --gulpfile test/lib/template-compiler/gulpfile.js",
    "test-lib-trim": "gulp build --gulpfile test/lib/trim/gulpfile.js",
    "test-tasks": "npm-run-all --serial test-tasks-*",
    "test-tasks-js": "gulp build --gulpfile test/tasks/js/gulpfile.js",
    "test-tasks-sass": "gulp build --gulpfile test/tasks/sass/gulpfile.js",
    "test-tasks-skin": "gulp build --gulpfile test/tasks/skin/gulpfile.js",
    "test-tasks-template": "cd test/tasks/template && gulp build --gulpfile gulpfile.js --cwd .",
    "build": "npm-run-all --serial build-*",
    "build-blank": "cd starter/blank && gulp build --gulpfile ../../gulpfile.bloggerpack.js --cwd .",
    "build-bootstrap4": "cd starter/bootstrap4 && gulp build --gulpfile ../../gulpfile.bloggerpack.js --cwd .",
    "release": "npm-run-all --serial release-*",
    "release-change-version": "gulp change_version --gulpfile gulpfile.release.js",
    "release-zip-starter": "gulp zip_starter --gulpfile gulpfile.release.js"
  },
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "cross-spawn": "^7.0.3",
    "del": "^5.1.0",
    "eslint-plugin-import": "^2.22.0",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-terser": "^1.3.2",
    "gulplog": "^1.0.0",
    "merge-options": "^2.0.0",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "nunjucks": "^3.2.2",
    "postcss-import": "^12.0.1",
    "prettier": "^2.1.2",
    "strip-css-comments": "^4.1.0",
    "strip-indent": "^3.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "through2": "^4.0.2",
    "undertaker-registry": "^1.0.1",
    "util": "^0.12.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "devDependencies": {
    "gulp-zip": "^5.0.2",
    "mocha": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "should": "^13.2.3"
  },
  "files": [
    "lib",
    "tasks",
    "bin.js",
    "gulpfile.bloggerpack.js"
  ]
}
