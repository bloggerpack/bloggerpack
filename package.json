{
  "name": "bloggerpack",
  "version": "1.0.0-beta.6",
  "version_current": "1.0.0-beta.6",
  "description": "A tool for develop Blogger theme.",
  "keywords": [
    "blogger",
    "blogspot",
    "theme",
    "template",
    "google"
  ],
  "author": "Igoy Nawamreh",
  "license": "MIT",
  "homepage": "https://github.com/bloggerpack/bloggerpack",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloggerpack/bloggerpack.git"
  },
  "bugs": {
    "url": "https://github.com/bloggerpack/bloggerpack/issues"
  },
  "bin": {
    "bloggerpack": "bin.js"
  },
  "scripts": {
    "test": "npm-run-all --serial test-lib test-tasks && mocha",
    "test-lib": "npm-run-all --serial test-lib-*",
    "test-lib-extract": "gulp build --gulpfile test/lib/extract/gulpfile.js",
    "test-lib-skin-ib": "gulp build --gulpfile test/lib/skin-import-beautifier/gulpfile.js",
    "test-lib-template-c": "gulp build --gulpfile test/lib/template-compiler/gulpfile.js",
    "test-lib-trim": "gulp build --gulpfile test/lib/trim/gulpfile.js",
    "test-tasks": "npm-run-all --serial test-tasks-*",
    "test-tasks-js": "gulp build --gulpfile test/tasks/js/gulpfile.js",
    "test-tasks-sass": "gulp build --gulpfile test/tasks/sass/gulpfile.js",
    "test-tasks-skin": "gulp build --gulpfile test/tasks/skin/gulpfile.js",
    "test-tasks-template": "cd test/tasks/template && gulp build --gulpfile gulpfile.js --cwd .",
    "build-starters": "npm-run-all --serial build-starters-*",
    "build-starters-blank": "cd starters/blank && gulp build --gulpfile ../../gulpfile.bloggerpack.js --cwd .",
    "build-starters-bootstrap4": "cd starters/bootstrap4 && gulp build --gulpfile ../../gulpfile.bloggerpack.js --cwd .",
    "release": "npm-run-all --serial release-change-version deps-install release-zip-starters",
    "release-change-version": "gulp change_version --gulpfile gulpfile.dev.js",
    "release-zip-starters": "gulp zip_starters --gulpfile gulpfile.dev.js",
    "deps-install": "npm-run-all --serial deps-install-*",
    "deps-install-blank": "cd starters/blank && npm install",
    "deps-install-bootstrap4": "cd starters/bootstrap4 && npm install"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/stream": "^1.1.0",
    "@taufik-nurrohman/quote": "0.0.7",
    "autoprefixer": "^10.0.1",
    "cross-spawn": "^7.0.3",
    "del": "^6.0.0",
    "fibers": "^5.0.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-terser": "^1.4.0",
    "merge-options": "^3.0.3",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "nunjucks": "^3.2.2",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "prettier": "^2.1.2",
    "rollup": "^2.34.1",
    "strip-css-comments": "^4.1.0",
    "strip-indent": "^3.0.0",
    "stylelint": "^13.7.2",
    "through2": "^4.0.2",
    "undertaker-registry": "^1.0.1",
    "util": "^0.12.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "devDependencies": {
    "gulp-zip": "^5.0.2",
    "mocha": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "should": "^13.2.3"
  },
  "files": [
    "lib",
    "tasks",
    "bin.js",
    "config.js",
    "gulpfile.bloggerpack.js"
  ]
}
